///////////////////////Constructor
void main(){
  var characterImformation = CharacterManager();
  var fromCharacter = Character("onethedk","warrior");
  characterImformation.add(fromCharacter);
}


class CharacterManager{
  void add(Character character){
    print("nick: " + character.nick);
    print("job: " + character.job);
  }
}

class Character{
  var nick;
  var job;
  Character(var nick, var job){
    this.nick = nick;
    this.job = job;
}
  
}

